{
	"format_version": "1.8.0",
	"animations": {
		"animation.frosty.baby_transform": {
			"loop": true,
			"bones": {
				"head": {
					"scale": 1.5
				}
			}
		},
		"animation.frosty.get_in_bed": {
			"loop": true,
			"bones": {
				"body": {
					"position": [0, 1, -15],
					"rotation": [-90, 0, 0]
				}
			}
		},
		"animation.frosty.move": {
			"anim_time_update": "query.modified_distance_moved",
			"loop": true,
			"bones": {
				"leg0": {
					"rotation": ["(math.cos(query.anim_time * 38.17) * 40.0) - this", "-this", 0]
				},
				"leg1": {
					"rotation": ["(math.cos(query.anim_time * 38.17 + 180) * 40.0) - this", "-this", 0]
				}
			}
		},
		"animation.frosty.raise_arms": {
			"loop": true,
			"bones": {
				"arms": {
					"rotation": ["variable.raise_arms * -15.0", 0, 0]
				}
			}
		}
	}
}